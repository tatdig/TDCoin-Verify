{
  "name": "tatdig/docsierify",
  "description": "Document Issue, Verification using TDCoin blockchain",
  "type": "library",
  "homepage": "https://www.tdcoincore.org/",
  "license": "Unlicense",
  "authors": [
    {
      "name": "Ayrat V Zakir",
      "homepage": "https://www.tdcoincore.org/",
      "role": "Author"
    }
